
sub main(): void
{
    // Will print 0, 1, 2, 3, ...
    foreach (const i in 0..10) {
        std::println("%d", i);
    }

    // Ranges may be used to initialize arrays quickly:
    const arr = [1..10];

    // Arrays may be iterated using iterators:
    foreach (const i in arr.iter()) {
        std::println("%d", i);
    }
}
